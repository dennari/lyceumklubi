---
layout: layout-outermost.hbs
---
<div class="main-logo-container" id="mobile-logo">

	<a href="./index.html">
	  <img class="brand" src="/{{rev "assets/logo.png"}}"/>
	</a>


	<a href="./index.html" class="logo">
	  <span class="line1">{{#tr "helsinkis"}}{{capitalizeFirst translated}}{{/tr}}</span><br/>
	  <span class="line2">{{#tr "lyceumclub"}}{{capitalizeFirst translated}}{{/tr}}</span>
	</a>

</div>


<article>
	<div 
		class="hero-unit" 
		style="background-image: url('/{{rev "assets/boat_at_night.jpg"}}');">
	</div>
</article>
 

{{#withSort pages "data.order"}}
	{{#is isCurrentPage false}}
	<article>
			<h1 id="{{data.key}}">{{#tr data.key}}{{capitalizeFirst translated}}{{/tr}}</h1>
		{{#markdown }}	
			{{{page}}}
		{{/markdown}}
	</article>
	{{/is}}
{{/withSort}}